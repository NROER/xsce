#
# School server 8 DHCP Server Configuration file.
#
ddns-update-style interim;
#ignore client-updates;

option domain-name "@@BASEDNSNAME@@";
option domain-name-servers      172.18.0.1;
option ntp-servers		172.18.0.1;

subnet 172.18.74.0 netmask 255.255.254.0 {
	option routers			172.18.74.1;
	option subnet-mask		255.255.254.0;
	option broadcast-address	172.18.75.255;
	range                           172.18.74.2 172.18.75.254;
	default-lease-time	600;
	max-lease-time 		1800;
}

subnet 172.18.76.0 netmask 255.255.254.0 {
	option routers			172.18.76.1;
	option subnet-mask		255.255.254.0;
	option broadcast-address	172.18.77.255;
	range                           172.18.76.2 172.18.76.254;
	default-lease-time	600;
	max-lease-time 		1800;
}

subnet 172.18.78.0 netmask 255.255.254.0 {
	option routers			172.18.78.1;
	option subnet-mask		255.255.254.0;
	option broadcast-address	172.18.79.255;
	range                           172.18.78.2 172.18.79.254;
	default-lease-time	600;
	max-lease-time 		1800;
}
