##
## This is the 'conventional wlan' interface
## of the wlanX / mshX pair that the Marvell
## mesh IF appears as...
##
###
### NOTE: This file will be _replaced_ in xs-config updates
###
DEVICE=wlan2
TYPE=Wireless
MODE=ad-hoc
CHANNEL=6
ESSID=school-mesh-2
ONBOOT=yes
BOOTPROTO=none
IPV6INIT=no
HOTPLUG=yes
NM_CONTROLLED=no
LINKDELAY=2
